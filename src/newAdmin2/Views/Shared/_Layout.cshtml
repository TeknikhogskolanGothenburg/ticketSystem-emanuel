
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Admin</title>
    <link rel="stylesheet" href="~/css/Layout.css" />
    <script src="@Url.Content("~/js/DragDrop/jquery.filedrop.js")" type="text/javascript"></script>

</head>
<body>
    <header>
        <div id="navbar">
            <ul>
                <li>@Html.ActionLink(linkText: "Eng/sv", actionName: "EngSv", controllerName: "Home")</li>
                    @if(EngSve.IsEng) {<li>@Html.ActionLink(linkText: "Add New Product", actionName: "AddProd", controllerName: "Home")</li> }
                    @if(!EngSve.IsEng){<li>@Html.ActionLink(linkText: "Lägg till produkt", actionName: "AddProd", controllerName: "Home")</li> }
                    @if(EngSve.IsEng){<li>@Html.ActionLink(linkText: "Find Order", actionName: "FindPurchase", controllerName: "Home")</li> }
                    @if(!EngSve.IsEng){<li>@Html.ActionLink(linkText: "Hitta Order", actionName: "FindPurchase", controllerName: "Home")</li> }           
            </ul>
        </div>
    </header>
    @RenderBody()


</body>
</html>

